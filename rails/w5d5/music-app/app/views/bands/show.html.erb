<h1>Band name: <%= @band.name %></h1>
<ul>
  <% @band.albums.each do |album| %>
      <a href="<%= album_url(album) %>"> Go to <%= album.name %></a>
  <% end %>
</ul>
<ul> 
  <li><a href="<%= new_band_album_url(@band) %>">Create New Album!</a></li>

  <li><a href="<%= edit_band_url(@band) %>">Edit Band!</a></li>

  <li>
    <form action="<%= band_url(@band) %>" method="post">
    <input type="hidden" name="_method" value="delete">
      <%= auth_token %>
      <button>Destroy Band</button>
    </form>
  </li>
</ul>